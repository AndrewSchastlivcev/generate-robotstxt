{
    "name": "generate-robotstxt",
    "version": "4.0.0",
    "description": "Awesome generator robots.txt",
    "author": "itgalaxy <development@itgalaxy.company>",
    "contributors": [
        {
            "name": "Alexander Krasnoyarov",
            "email": "alexander.krasnoyarov@itgalaxy.company",
            "url": "https://vk.com/sterling_archer"
        }
    ],
    "repository": {
        "type": "https",
        "url": "https://github.com/itgalaxy/generate-robotstxt"
    },
    "keywords": [
        "robotstxt",
        "robots.txt",
        "generate",
        "robots-txt",
        "user-agent",
        "police"
    ],
    "bugs": {
        "url": "https://github.com/itgalaxy/generate-robotstxt/issues"
    },
    "main": "dist/index.js",
    "bin": "dist/cli.js",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "dependencies": {
        "meow": "^3.3.0",
        "resolve-from": "^2.0.0"
    },
    "devDependencies": {
        "ava": "^0.16.0",
        "babel-cli": "^6.10.0",
        "babel-core": "^6.4.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.14.0",
        "coveralls": "^2.11.6",
        "npm-run-all": "^3.0.0",
        "npmpub": "^3.1.0",
        "eslint": "^3.4.0",
        "eslint-plugin-ava": "^3.0.0",
        "eslint-plugin-import": "^1.14.0",
        "eslint-plugin-itgalaxy": "^13.0.0",
        "eslint-plugin-jsx-a11y": "^2.2.0",
        "eslint-plugin-lodash": "^1.10.0",
        "eslint-plugin-node": "^2.0.0",
        "eslint-plugin-promise": "^2.0.0",
        "eslint-plugin-react": "^6.2.0",
        "eslint-plugin-xo": "^0.5.0",
        "execa": "^0.4.0",
        "remark-cli": "^2.0.0",
        "remark-lint": "^5.0.0",
        "remark-preset-lint-itgalaxy": "^1.0.0",
        "rimraf": "^2.5.2",
        "nyc": "^8.0.0",
        "tempfile": "^1.1.0",
        "path-exists": "^3.0.0"
    },
    "scripts": {
        "coveralls": "nyc report --reporter=text-lcov | coveralls",
        "lint:eslint": "eslint . --ignore-path .gitignore",
        "lint:remark": "remark . --ignore-path .gitignore --frail --quiet",
        "lint": "npm-run-all -l --parallel lint:*",
        "pretest": "npm run lint",
        "ava": "nyc ava --verbose \"src/**/__tests__/*.js\"",
        "test": "npm run ava",
        "prebuild": "rimraf dist",
        "build": "babel src --out-dir dist --ignore /__tests__/",
        "prepublish": "npm run build",
        "release": "npmpub"
    },
    "nyc": {
        "exclude": [
            "**/__tests__"
        ]
    },
    "ava": {
        "require": "babel-register",
        "concurrency": 5,
        "babel": "inherit"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    },
    "remarkConfig": {
        "presets": [
            "remark-preset-lint-itgalaxy"
        ]
    },
    "eslintConfig": {
        "rules": {
            "node/shebang": "off"
        },
        "extends": [
            "plugin:itgalaxy/ava",
            "plugin:itgalaxy/esnext",
            "plugin:itgalaxy/node"
        ],
        "root": true
    }
}
